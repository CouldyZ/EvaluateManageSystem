<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  
  <servlet>
    <servlet-name>ServletLogin</servlet-name>
    <servlet-class>com.xt.servlet.ServletLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletRegister</servlet-name>
    <servlet-class>com.xt.servlet.ServletRegister</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletEvaluate</servlet-name>
    <servlet-class>com.xt.servlet.ServletEvaluate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletAlterStuInfo</servlet-name>
    <servlet-class>com.xt.servlet.ServletAlterStuInfo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletChaPassword</servlet-name>
    <servlet-class>com.xt.servlet.ServletChaPassword</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletShow</servlet-name>
    <servlet-class>com.xt.servlet.ServletShow</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletDelete</servlet-name>
    <servlet-class>com.xt.servlet.ServletDelete</servlet-class>
  </servlet>


  





  <servlet-mapping>
    <servlet-name>ServletLogin</servlet-name>
    <url-pattern>/ServletLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletRegister</servlet-name>
    <url-pattern>/ServletRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletEvaluate</servlet-name>
    <url-pattern>/ServletEvaluate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletAlterStuInfo</servlet-name>
    <url-pattern>/ServletAlterStuInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletChaPassword</servlet-name>
    <url-pattern>/ServletChaPassword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletShow</servlet-name>
    <url-pattern>/ServletShow</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletDelete</servlet-name>
    <url-pattern>/ServletDelete</url-pattern>
  </servlet-mapping>
	
  
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
